#include "apq8053-lite-dragon.dtsi"

&mdss_dsi0 {
	qcom,ext-vdd-gpio = <&tlmm 100 0>;
	qcom,platform-bklight-en-gpio = <&tlmm 95 0>;

	qcom,platform-lane-config = [00 00 ff 0f
				00 00 ff 0f
				00 00 ff 0f
				00 00 ff 0f
				00 00 ff 8f];
};

&eeprom0 {
	gpios = <&tlmm 26 0>,
		<&tlmm 40 0>,
		<&tlmm 118 0>,
		<&tlmm 119 0>,
		<&tlmm 39 0>;
	qcom,gpio-vdig = <3>;
	qcom,gpio-vana = <4>;
	qcom,gpio-req-tbl-num = <0 1 2 3 4>;
	qcom,gpio-req-tbl-flags = <1 0 0 0 0>;
	qcom,gpio-req-tbl-label = "CAMIF_MCLK0",
			"CAM_RESET0",
			"CAM_VDIG",
			"CAM_VANA",
			"CAM_STANDBY0";
};

&camera0 {
	qcom,mount-angle = <270>;
	gpios = <&tlmm 26 0>,
		<&tlmm 40 0>,
		<&tlmm 39 0>,
		<&tlmm 118 0>,
		<&tlmm 119 0>;
	qcom,gpio-vdig = <3>;
	qcom,gpio-vana = <4>;
	qcom,gpio-req-tbl-num = <0 1 2 3 4>;
	qcom,gpio-req-tbl-flags = <1 0 0 0 0>;
	qcom,gpio-req-tbl-label = "CAMIF_MCLK0",
			"CAM_RESET0",
			"CAM_STANDBY0",
			"CAM_VDIG",
			"CAM_VANA";
};

&camera1 {
	qcom,mount-angle = <270>;
};

&camera2{
	qcom,mount-angle = <270>;
};
